{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers": [{
      "source": "**/*",
      "headers": [{
        "key": "X-Robots-Tag",
        "value": "noindex, nofollow"
      }]
    }]
  }
}
